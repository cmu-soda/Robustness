range LINES = 1..2

NURSE_ENVIRON = (
  plug_in -> turn_on -> line[i:LINES].set_rate -> line[i].enter_value -> line[i].press_set -> line[i].connect_set ->
  line[i].purge_air -> line[i].lock_line -> line[i].confirm_settings -> line[i].lock_unit ->
  line[i].dispense_main_med_flow -> line[i].flow_complete -> line[i].unlock_unit -> turn_off -> NURSE_ENVIRON
).